<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.db.dao.MemberMapper">
	<select id="selectMemberList" resultType="member">
		select * from member_info
	</select>
	
	<insert id="insertMember" parameterType="member">
		insert into member_info values(#{id}, #{passwd}, #{name}, #{birth}, #{phone}, #{email})
	</insert>
	
	<insert id="insertMemberDetail" parameterType="member">
		insert into membertest values(#{id})
	</insert>
	
	<select id="selectMemberById" parameterType="string" resultType="member">
		select * from member where id = #{id}
	</select>
</mapper>